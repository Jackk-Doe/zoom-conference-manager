public class Solution {
    public int[] TwoSum(int[] nums, int target) 
    {
       
          for (int i = 0; i<nums.Length; i++)
          
          {
              for(int j = i+1; i<nums.Length; j++)
              {
                  if(nums[i]+nums[j]==target)
                  {
                      return new int[] {i,j};
                  }
            
              }
                              
              }throw new Exception("Not found");
                              
           }
            
       }



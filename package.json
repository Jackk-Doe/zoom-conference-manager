{
  "name": "zoom-conference-manager",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "start:client": "nx serve client -c dev",
    "start:api": "nx serve api -c dev",
    "start:app": "nx run-many --target=serve --projects=client,api -c dev",
    "lint": "nx lint",
    "lint:client": "nx lint client",
    "lint:api": "nx lint api",
    "lint:all": "nx run-many --target=lint --all",
    "lint:affected": "nx affected:lint",
    "format:all": "prettier --write \".\"",
    "test": "nx test",
    "test:client": "nx test client",
    "test:api": "nx test api",
    "test:all": "nx run-many --target=test --all",
    "test:affected": "nx affected:test",
    "build:staging": "nx run-many --target=build --projects=client,api -c staging",
    "build:production": "nx run-many --target=build --projects=client,api -c production",
    "build:image": "docker build . -t zoom-manager-base-image:nx-base && docker compose build",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.3",
    "@mui/x-date-pickers": "^5.0.0-alpha.2",
    "chalk": "^4.1.2",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.0",
    "express": "4.17.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.30.0",
    "react-router-dom": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.0.0",
    "typeorm": "^0.3.6",
    "winston": "^3.7.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.7.2",
    "@nrwl/cli": "13.8.8",
    "@nrwl/cypress": "13.8.8",
    "@nrwl/eslint-plugin-nx": "13.8.8",
    "@nrwl/express": "13.8.8",
    "@nrwl/jest": "13.8.8",
    "@nrwl/linter": "13.8.8",
    "@nrwl/node": "13.8.8",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "13.8.8",
    "@nrwl/tao": "13.8.8",
    "@nrwl/web": "13.8.8",
    "@nrwl/workspace": "13.8.8",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "16.11.7",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@typescript-eslint/eslint-plugin": "~5.13.0",
    "@typescript-eslint/parser": "~5.10.0",
    "babel-jest": "27.2.3",
    "cypress": "^9.1.0",
    "eslint": "~8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.3",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "lint-staged": "^12.3.7",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint:all",
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}
